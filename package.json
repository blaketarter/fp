{
  "name": "simple-fp",
  "version": "0.5.0",
  "description": "A simple, small, no dependency functional programming library",
  "keywords": [
    "functional",
    "simple",
    "small",
    "browser",
    "server",
    "client",
    "util",
    "fp"
  ],
  "main": "fp.js",
  "scripts": {
    "build": "rollup src/index.js --output.format umd --name \"fp\" --output.file dist/index.js",
