{
  "name": "simple-fp",
  "version": "2.0.0",
  "description": "A simple, small, no dependency functional programming library",
  "keywords": [
    "functional",
    "simple",
    "small",
    "browser",
    "server",
    "client",
    "util",
    "fp"
  ],
  "main": "dist/index.js",
  "scripts": {
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "rollup": "rollup build/index.js --output.format umd --name \"fp\" --output.file dist/index.js",
    "rollup:watch": "rollup build/index.js --output.format umd --name \"fp\" --output.file dist/index.js -w",
    "build": "npm run tsc && npm run rollup",
    "build:watch": "concurrently \"npm run tsc:watch\" \"npm run rollup:watch\"",
    "test": "NODE_ENV=test npm run build && jest",
    "test:watch": "NODE_ENV=test npm run test -- --watch"
  },
  "author": "Blake Tarter <blakertarter@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/blaketarter/simple-fp"
  },
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rollup": "^2.0.6",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
