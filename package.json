{
  "name": "simple-fp",
  "version": "0.6.0",
  "description": "A simple, small, no dependency functional programming library",
  "keywords": [
    "functional",
    "simple",
    "small",
    "browser",
    "server",
    "client",
    "util",
    "fp"
  ],
  "main": "./dist/fp.js",
  "scripts": {
    "build": "rollup src/fp.js --format umd --name fp --output dist/fp.js",
    "compress": "uglifyjs --compress -- dist/fp.js -o dist/fp.min.js",
    "test": "npm run lint && npm run jest",
    "jest": "node ./node_modules/jest/bin/jest.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js src"
  },
  "author": "Blake Tarter <blakertarter@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/blaketarter/simple-fp"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^18.1.0",
    "rollup": "^0.47.6",
    "uglify": "^0.1.5"
  }
}
