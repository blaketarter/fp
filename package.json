  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.7.0",
    "jest": "^18.1.0",
    "rollup": "^0.55.1"
  },
  "dependencies": {}
}
